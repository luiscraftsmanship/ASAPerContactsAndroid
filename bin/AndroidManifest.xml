<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.livewatch.asapercontactsandroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    
      <uses-permission android:name="android.permission.READ_CONTACTS" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_spotlight"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MessageASAPerActivity"
            android:theme="@style/Theme.Sherlock.Light" >
        </activity>
        <activity
            android:name=".HomeActivity"
            android:theme="@style/Theme.Sherlock.Light" >
        </activity>
        <activity
            android:name=".SendFeedbackActivity"
            android:theme="@style/Theme.Sherlock.Light" >
        </activity>
        <activity
            android:name=".SelectContactsActivity"
            android:theme="@style/Theme.Sherlock.Light" >
        </activity>
        <activity
            android:name=".GroupsActivity"
            android:theme="@style/Theme.Sherlock.Light" >
        </activity>
        <activity
            android:name=".ContactListActivity"
            android:theme="@style/Theme.Sherlock.Light" >
        </activity>
    </application>

</manifest>


